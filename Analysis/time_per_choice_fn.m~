function [time_per_choice]  = time_per_choice_fn(x_y_time_color,cps)
    time_per_choice = [];
    ct = 0;
    for i = 1:length(cps)
        start_pt = [];
        end_pt = [];
        no_match = 1;
        kk = 0;
        while no_match == 1
            kk = kk+1;
            if sum(x_y_time_color.color(cps(i)-kk,:) == [0 0.5 0])==3
                no_match = 0;
                start_pt = x_y_time_color.time(cps(i)-kk);
                
                